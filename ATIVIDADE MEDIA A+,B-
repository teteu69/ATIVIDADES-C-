#include <stdio.h>

int main(){
	
	    float nota;
	    char nome;
	    
	    printf("didigite seu nome: ");
	    scanf("%s", &nome);
	    printf("didigite sua nota: ");
	    scanf("%f", &nota);
	    
	if(nota >= 0 && nota <= 2.9){
			printf("nota muito a baixo da media E - : %f", nota );
	}
	else if(nota>= 3 && nota <= 4.9){
		
		printf("nota a baixo da media D - : %f", nota );
	}
	else if(nota>= 5 && nota <= 6.9){
		
		printf("nota na media C - : %f", nota );
	}
	else if(nota>= 7 && nota <= 8.9){
		
		printf("nota a cima da media B : %f", nota );
	}
	else if(nota>= 9 && nota <= 10){
		
		printf("nota perfeita A +: %f", nota );
		
		printf("NOME: %s", nome);
	}
}
